extends layout

block content
	h2 Upload
	form(enctype='multipart/form-data', method='post', action='/upload')
		button(type='submit') Upload
		label(for='img')
			input(type='file', name='img').img
	p Outside form
	if vars.err
		p Upload Failed, The File Already Exists
	if vars.url
		a(href=vars.url) #{vars.url}
